********************************************************************************************************-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:mast_mdl="http://mast.unican.es/xmlmast/model" targetNamespace="http://mast.unican.es/xmlmast/model" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!-- SIMPLE TYPES -->
	<xs:simpleType name="Identifier">
		<xs:restriction base="xs:NCName">
			<xs:pattern value="([a-z]|[A-Z])([a-z]|[A-Z]|[0-9]|.|_)*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Identifier_Ref">
		<xs:restriction base="mast_mdl:Identifier"/>
	</xs:simpleType>
	<xs:simpleType name="Identifier_Ref_List">
		<xs:restriction base="xs:NMTOKENS"/>
	</xs:simpleType>
	<xs:simpleType name="Priority">
		<xs:restriction base="xs:nonNegativeInteger"/>
	</xs:simpleType>
	<xs:simpleType name="Interrupt_Priority">
		<xs:restriction base="xs:nonNegativeInteger"/>
	</xs:simpleType>
	<xs:simpleType name="Preemption_Level">
		<xs:restriction base="xs:nonNegativeInteger"/>
	</xs:simpleType>
	<xs:simpleType name="Time">
		<xs:restriction base="xs:double">
			<xs:minInclusive value="0.0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Normalized_Execution_Time">
		<xs:restriction base="xs:double">
			<xs:minInclusive value="0.0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Absolute_Time">
		<xs:restriction base="xs:double">
			<xs:minInclusive value="0.0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Bit_Count">
		<xs:restriction base="xs:nonNegativeInteger"/>
	</xs:simpleType>
	<xs:simpleType name="Throughput">
		<xs:restriction base="xs:double">
			<xs:minInclusive value="0.0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Float">
		<xs:restriction base="xs:double"/>
	</xs:simpleType>
	<xs:simpleType name="Natural">
		<xs:restriction base="xs:nonNegativeInteger"/>
	</xs:simpleType>
	<xs:simpleType name="Positive">
		<xs:restriction base="xs:positiveInteger"/>
	</xs:simpleType>
	<xs:simpleType name="Percentage">
		<xs:restriction base="xs:float">
			<xs:minInclusive value="0.0"/>
			<xs:maxInclusive value="100.0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Assertion">
		<xs:restriction base="xs:string">
			<xs:enumeration value="YES"/>
			<xs:enumeration value="NO"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Date_Time">
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="Pathname">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="Transmission_Type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="SIMPLEX"/>
			<xs:enumeration value="HALF_DUPLEX"/>
			<xs:enumeration value="FULL_DUPLEX"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Distribution_Type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="UNIFORM"/>
			<xs:enumeration value="POISSON"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Request_Policy_Type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="PRIORITY"/>
			<xs:enumeration value="FIFO"/>
			<xs:enumeration value="LIFO"/>
			<xs:enumeration value="SCAN"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Delivery_Policy_Type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="SCAN"/>
			<xs:enumeration value="RANDOM"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Queueing_Policy_Type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="FIFO"/>
			<xs:enumeration value="PRIORITY"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Priority_Inheritance_Protocol">
		<xs:restriction base="xs:string">
			<xs:enumeration value="STRICT"/>
			<xs:enumeration value="POSIX"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- TIMING OBJECTS -->
	<xs:complexType name="Clock_Synchronization_Object">
		<xs:attribute name="Name" type="mast_mdl:Identifier" use="required"/>
		<xs:attribute name="Precision" type="mast_mdl:Time" use="optional"/>
	</xs:complexType>
	<xs:complexType name="Ticker">
		<xs:attribute name="Name" type="mast_mdl:Identifier" use="required"/>
		<xs:attribute name="Max_Overhead" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Overhead" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Overhead" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Period" type="mast_mdl:Time" use="optional"/>
		<xs:attribute name="Is_Locally_Synchronized" type="mast_mdl:Assertion" use="optional"/>
		<xs:attribute name="Precision" type="mast_mdl:Time" use="optional"/>
	</xs:complexType>
	<xs:complexType name="Alarm_Clock">
		<xs:attribute name="Name" type="mast_mdl:Identifier" use="required"/>
		<xs:attribute name="Max_Overhead" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Overhead" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Overhead" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Is_Locally_Synchronized" type="mast_mdl:Assertion" use="optional"/>
		<xs:attribute name="Precision" type="mast_mdl:Time" use="optional"/>
	</xs:complexType>
	<xs:complexType name="Timer_Ref">
		<xs:attribute name="Name" type="mast_mdl:Identifier_Ref"/>
	</xs:complexType>
	<!--  PROCESSING RESOURCES -->
	<!-- Computing resources -->
	<xs:complexType name="Regular_Processor">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="Timer" type="mast_mdl:Timer_Ref"/>
		</xs:sequence>
		<xs:attribute name="Name" type="mast_mdl:Identifier" use="required"/>
		<xs:attribute name="Speed_Factor" type="mast_mdl:Float" use="optional"/>
		<xs:attribute name="System_Timer" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Synchronization_Source" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Max_Interrupt_Priority" type="mast_mdl:Interrupt_Priority" use="optional"/>
		<xs:attribute name="Min_Interrupt_Priority" type="mast_mdl:Interrupt_Priority" use="optional"/>
		<xs:attribute name="Worst_ISR_Switch" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_ISR_Switch" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Best_ISR_Switch" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
	</xs:complexType>
	<!-- Networks -->
	<xs:complexType name="Packet_Based_Network">
		<xs:sequence minOccurs="0">
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Packet_Driver" type="mast_mdl:Packet_Driver"/>
				<xs:element name="Character_Packet_Driver" type="mast_mdl:Character_Packet_Driver"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="Name" type="mast_mdl:Identifier" use="required"/>
		<xs:attribute name="Speed_Factor" type="mast_mdl:Float" use="optional"/>
		<xs:attribute name="Synchronization_Source" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Throughput" type="mast_mdl:Throughput" use="optional"/>
		<xs:attribute name="Transmission_Kind" type="mast_mdl:Transmission_Type" use="optional"/>
		<xs:attribute name="Max_Blocking" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Max_Packet_Size" type="mast_mdl:Bit_Count" use="optional"/>
		<xs:attribute name="Min_Packet_Size" type="mast_mdl:Bit_Count" use="optional"/>
	</xs:complexType>
	<xs:complexType name="RTEP_Network">
		<xs:sequence minOccurs="0">
			<xs:element name="RTEP_Packet_Driver" type="mast_mdl:RTEP_Packet_Driver" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Name" type="mast_mdl:Identifier" use="required"/>
		<xs:attribute name="Speed_Factor" type="mast_mdl:Float" use="optional"/>
		<xs:attribute name="Synchronization_Source" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Throughput" type="mast_mdl:Throughput" use="optional"/>
		<xs:attribute name="Transmission_Kind" type="mast_mdl:Transmission_Type" use="optional"/>
		<xs:attribute name="Max_Blocking" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Max_Packet_Size" type="mast_mdl:Bit_Count" use="optional"/>
		<xs:attribute name="Min_Packet_Size" type="mast_mdl:Bit_Count" use="optional"/>
		<xs:attribute name="Number_Of_Stations" type="mast_mdl:Positive" use="required"/>
		<xs:attribute name="Token_Delay" type="mast_mdl:Time" use="optional"/>
		<xs:attribute name="Failure_Timeout" type="mast_mdl:Time" use="optional"/>
		<xs:attribute name="Token_Transmission_Retries" type="mast_mdl:Natural" use="optional"/>
		<xs:attribute name="Packet_Transmission_Retries" type="mast_mdl:Natural" use="optional"/>
		<xs:attribute name="Arbitration_Token_Size" type="mast_mdl:Bit_Count" use="optional"/>
		<xs:attribute name="Transmitt_Token_Size" type="mast_mdl:Bit_Count" use="optional"/>
	</xs:complexType>
	<xs:complexType name="AFDX_Link">
		<xs:sequence minOccurs="0">
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Packet_Driver" type="mast_mdl:Packet_Driver"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="Name" type="mast_mdl:Identifier" use="required"/>
		<xs:attribute name="Speed_Factor" type="mast_mdl:Float" use="optional"/>
		<xs:attribute name="Synchronization_Source" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Throughput" type="mast_mdl:Throughput" use="optional"/>
		<xs:attribute name="Max_Packet_Size" type="mast_mdl:Bit_Count" use="optional"/>
		<xs:attribute name="Min_Packet_Size" type="mast_mdl:Bit_Count" use="optional"/>
		<xs:attribute name="Max_HW_Tx_Latency" type="mast_mdl:Time" use="optional"/>
		<xs:attribute name="Avg_HW_Tx_Latency" type="mast_mdl:Time" use="optional"/>
		<xs:attribute name="Min_HW_Tx_Latency" type="mast_mdl:Time" use="optional"/>
		<xs:attribute name="Max_HW_Rx_Latency" type="mast_mdl:Time" use="optional"/>
		<xs:attribute name="Avg_HW_Rx_Latency" type="mast_mdl:Time" use="optional"/>
		<xs:attribute name="Min_HW_Rx_Latency" type="mast_mdl:Time" use="optional"/>
		<xs:attribute name="Ethernet_Overhead" type="mast_mdl:Bit_Count" fixed="180"/>
		<xs:attribute name="Protocol_Overhead" type="mast_mdl:Bit_Count" fixed="376"/>
	</xs:complexType>
	<!-- Drivers-->
	<xs:complexType name="Packet_Driver">
		<xs:attribute name="Packet_Send_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Packet_Receive_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Packet_Server" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Message_Partitioning" type="mast_mdl:Assertion" use="optional"/>
		<xs:attribute name="High_Utilization_Mode" type="mast_mdl:Assertion" use="optional"/>
	</xs:complexType>
	<xs:complexType name="Character_Packet_Driver">
		<xs:attribute name="Packet_Send_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Packet_Receive_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Character_Send_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Character_Receive_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Packet_Server" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Character_Server" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Character_Transmission_Time" type="mast_mdl:Time" use="optional"/>
		<xs:attribute name="Message_Partitioning" type="mast_mdl:Assertion" use="optional"/>
		<xs:attribute name="High_Utilization_Mode" type="mast_mdl:Assertion" use="optional"/>
	</xs:complexType>
	<xs:complexType name="RTEP_Packet_Driver">
		<xs:attribute name="Packet_Send_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Packet_Receive_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Packet_Server" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Packet_Interrupt_Server" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Packet_ISR_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Token_Check_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Token_Manage_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Packet_Discard_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Token_Retransmission_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Packet_Retransmission_Operation" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Message_Partitioning" type="mast_mdl:Assertion" use="optional"/>
		<xs:attribute name="High_Utilization_Mode" type="mast_mdl:Assertion" use="optional"/>
	</xs:complexType>
	<!--Switches & routers  -->
	<xs:complexType name="Regular_Switch">
		<xs:attribute name="Name" type="mast_mdl:Identifier" use="required"/>
		<xs:attribute name="Speed_Factor" type="mast_mdl:Float" use="optional"/>
		<xs:attribute name="Synchronization_Source" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Max_Fixed_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Fixed_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Fixed_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Max_Variable_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Variable_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Variable_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Max_Delivery_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Delivery_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Delivery_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
	</xs:complexType>
	<xs:complexType name="AFDX_Switch">
		<xs:attribute name="Name" type="mast_mdl:Identifier" use="required"/>
		<xs:attribute name="Speed_Factor" type="mast_mdl:Float" use="optional"/>
		<xs:attribute name="Synchronization_Source" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Max_Fixed_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Fixed_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Fixed_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Max_Variable_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Variable_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Variable_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Max_Delivery_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Delivery_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Delivery_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
	</xs:complexType>
	<xs:complexType name="Regular_Router">
		<xs:attribute name="Name" type="mast_mdl:Identifier" use="required"/>
		<xs:attribute name="Speed_Factor" type="mast_mdl:Float" use="optional"/>
		<xs:attribute name="Synchronization_Source" type="mast_mdl:Identifier_Ref" use="optional"/>
		<xs:attribute name="Max_Fixed_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Fixed_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Fixed_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Max_Variable_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Variable_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Variable_Fork_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Max_Delivery_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Delivery_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Delivery_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Max_Fixed_Branch_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Fixed_Branch_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Fixed_Branch_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Max_Variable_Branch_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Variable_Branch_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Min_Variable_Branch_Latency" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
	</xs:complexType>
	<!-- SCHEDULERS -->
	<xs:complexType name="Primary_Scheduler">
		<xs:choice>
			<xs:element name="Fixed_Priority_Policy" type="mast_mdl:Fixed_Priority_Policy"/>
			<xs:element name="EDF_Policy" type="mast_mdl:EDF_Policy"/>
			<xs:element name="FP_Packet_Based_Policy" type="mast_mdl:FP_Packet_Based_Policy"/>
			<xs:element name="Timetable_Driven_Policy" type="mast_mdl:Timetable_Driven_Policy"/>
			<xs:element name="Timetable_Driven_Packet_Based_Policy" type="mast_mdl:Timetable_Driven_Packet_Based_Policy"/>
			<xs:element name="AFDX_Policy" type="mast_mdl:AFDX_Policy"/>
		</xs:choice>
		<xs:attribute name="Name" type="mast_mdl:Identifier" use="required"/>
		<xs:attribute name="Host" type="mast_mdl:Identifier_Ref" use="required"/>
	</xs:complexType>
	<xs:complexType name="Secondary_Scheduler">
		<xs:choice>
			<xs:element name="Fixed_Priority_Policy" type="mast_mdl:Fixed_Priority_Policy"/>
			<xs:element name="EDF_Policy" type="mast_mdl:EDF_Policy"/>
			<xs:element name="FP_Packet_Based_Policy" type="mast_mdl:FP_Packet_Based_Policy"/>
			<xs:element name="Timetable_Driven_Policy" type="mast_mdl:Timetable_Driven_Policy"/>
			<xs:element name="Timetable_Driven_Packet_Based_Policy" type="mast_mdl:Timetable_Driven_Packet_Based_Policy"/>
			<xs:element name="AFDX_Policy" type="mast_mdl:AFDX_Policy"/>
		</xs:choice>
		<xs:attribute name="Name" type="mast_mdl:Identifier" use="required"/>
		<xs:attribute name="Host" type="mast_mdl:Identifier_Ref" use="required"/>
	</xs:complexType>
	<!-- Scheduling policies -->
	<xs:complexType name="Fixed_Priority_Policy">
		<xs:attribute name="Worst_Context_Switch" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Context_Switch" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Best_Context_Switch" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Max_Priority" type="mast_mdl:Priority" use="optional"/>
		<xs:attribute name="Min_Priority" type="mast_mdl:Priority" use="optional"/>
	</xs:complexType>
	<xs:complexType name="EDF_Policy">
		<xs:attribute name="Worst_Context_Switch" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Context_Switch" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Best_Context_Switch" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
	</xs:complexType>
	<xs:complexType name="FP_Packet_Based_Policy">
		<xs:attribute name="Max_Priority" type="mast_mdl:Priority" use="optional"/>
		<xs:attribute name="Min_Priority" type="mast_mdl:Priority" use="optional"/>
		<xs:attribute name="Packet_Overhead_Max_Size" type="mast_mdl:Bit_Count"/>
		<xs:attribute name="Packet_Overhead_Min_Size" type="mast_mdl:Bit_Count"/>
		<xs:attribute name="Packet_Overhead_Avg_Size" type="mast_mdl:Bit_Count"/>
	</xs:complexType>
	<xs:complexType name="Timetable_Driven_Policy">
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="Partition" type="mast_mdl:Partition_Window"/>
		</xs:sequence>
		<xs:attribute name="MAF" type="mast_mdl:Time" use="required"/>
		<xs:attribute name="Worst_Context_Switch" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Avg_Context_Switch" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
		<xs:attribute name="Best_Context_Switch" type="mast_mdl:Normalized_Execution_Time" use="optional"/>
	</xs:complexType>
	<xs:complexType name="Timetable_Driven_Packet_Based_Policy">
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="Partition" type="mast_mdl:Partition_Window"/>
		</xs:sequence>
		<xs:attribute name="MAF" type="mast_mdl:Time" use="required"/>
	</xs:complexType>
	<xs:complexType name="AFDX_Policy"/>
	<xs:complexType name="Partition_Window">
		<xs:attribute name="Partition_Id" type="mast_mdl:Natural" use="required"/>
